<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frame1RowModel" type="com.soulanjo.app.modules.storiesandtweets.data.model.Frame1RowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearPostItem"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingEnd="@dimen/_16pxh" android:paddingStart="@dimen/_16pxh">
        <LinearLayout android:id="@+id/linearFollow"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageAlbertOconnor"
                android:layout_height="@dimen/_50pxh"
                android:layout_width="@dimen/_50pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse_12}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_12"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAlbertOConnor"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAlbertOConnor"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{frame1RowModel.txtAlbertOConnor}"
                    style="@style/txtIntersemibold20_1" tools:text="@string/lbl_albert_o_connor"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDuration"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{frame1RowModel.txtDuration}"
                    style="@style/txtInterregular14" tools:text="@string/lbl_4_hours_ago"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer1"
                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_border_name_deep_purple_a200_radius_16"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:gravity="center" android:id="@+id/btnFollow"
                android:layout_height="@dimen/_33pxv"
                android:layout_width="@dimen/_76pxh"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_8pxv"
                android:singleLine="true"
                android:text="@string/lbl_follow"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;deep_purple_A200&quot;}"
                android:textFontWeight="500"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_follow"/>
        </LinearLayout>
        <TextView android:id="@+id/txtIntroduceIAmA"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@{frame1RowModel.txtIntroduceIAmA}"
            style="@style/txtInterregular16" tools:text="@string/msg_introduce_i_am_a"/>
        <LinearLayout android:id="@+id/linearFourHundredEleven"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImage4"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_115pxh"
                android:layout_width="@dimen/_85pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_22}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_22"/>
            <ImageView android:id="@+id/imageImage5"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_115pxh"
                android:layout_width="@dimen/_85pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_25}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_25"/>
            <ImageView android:id="@+id/imageFourHundredEleven"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_115pxh"
                android:layout_width="@dimen/_85pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_41_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_41_1"/>
            <ImageView android:id="@+id/imageSixteen"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_115pxh"
                android:layout_width="@dimen/_85pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_16}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_16"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearZipcode"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImage"
                android:layout_height="@dimen/_17pxh"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtZipcode"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{frame1RowModel.txtZipcode}"
                style="@style/txtInterregular12_1" tools:text="@string/lbl_2200"/>
            <ImageView android:id="@+id/imageImage1"
                android:layout_height="@dimen/_18pxh"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_deep_purple_a200"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_deep_purple_a200"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEightHundred"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{frame1RowModel.txtEightHundred}"
                style="@style/txtInterregular12_1" tools:text="@string/lbl_800"/>
            <FrameLayout android:id="@+id/frameSpacer2"
                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
            <ImageView android:id="@+id/imageImage2"
                android:layout_height="@dimen/_25pxh"
                android:layout_width="@dimen/_54pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_profile_deep_purple_a200"
                tools:ignore="ContentDescription" tools:src="@drawable/img_profile_deep_purple_a200"/>
        </LinearLayout>
    </LinearLayout>
</layout>
