<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="liveVM" type="com.soulanjo.app.modules.live.data.viewmodel.LiveVM"/>
    </data>
    <FrameLayout android:id="@+id/frameComment"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearLive"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylenameprimary_1">
            <LinearLayout android:id="@+id/linearFourteen"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarAppBar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <ImageView android:id="@+id/imageCircleImage"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse_9}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_9"/>
                    <LinearLayout android:id="@+id/linearGroup20"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLucasAnna"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{liveVM.liveModel.txtLucasAnna}"
                            style="@style/txtIntersemibold20_2" tools:text="@string/lbl_lucas_anna"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThreeThousandFiveHundredSixtee"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{liveVM.liveModel.txtThreeThousandFiveHundredSixtee}"
                            style="@style/txtInterregular14_1" tools:text="@string/lbl_35_16"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_primary"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_primary"/>
                </androidx.appcompat.widget.Toolbar>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearCommentSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_29pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_29pxv" style="@style/groupStyle_1">
                <LinearLayout android:id="@+id/linearErlikheSweetCan"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearErlikheSweetCan1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <LinearLayout
                            android:id="@+id/linearErlikheSweetCan2"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/_2pxv"
                            android:paddingEnd="@dimen/_8pxh"
                            android:paddingStart="@dimen/_8pxh"
                            android:paddingTop="@dimen/_2pxv" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimary&quot;}cornerRadius">
                            <ImageView
                                android:id="@+id/imageCircleImage1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_45pxh"
                                android:layout_width="@dimen/_45pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse_9_45x45}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_9_45x45"/>
                            <TextView android:gravity="bottom"
                                android:id="@+id/txtErlikheSweetCan"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:text="@{liveVM.liveModel.txtErlikheSweetCan}"
                                style="@style/txtHeeboregular18" tools:text="@string/msg_erlikhe_sweet_can"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearDongKhuwanSoBeautiful"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_8pxh"
                            android:paddingStart="@dimen/_8pxh"
                            android:paddingTop="@dimen/_6pxv" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimary&quot;}cornerRadius">
                            <ImageView
                                android:id="@+id/imageCircleImage2"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_45pxh"
                                android:layout_width="@dimen/_45pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse_8_45x45}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_8_45x45"/>
                            <TextView android:gravity="center_vertical"
                                android:id="@+id/txtDongKhuwanSoBeautiful"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:text="@{liveVM.liveModel.txtDongKhuwanSoBeautiful}"
                                style="@style/txtInterregular18_1" tools:text="@string/msg_dong_khuwan_so_beautiful"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_width="@dimen/_98pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group_9142"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group_9142"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_name_gray_600_radius_25"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_write_a_comment"
                    android:id="@+id/etComment" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingHorizontal="@dimen/_16pxh"
                    android:paddingVertical="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@={liveVM.liveModel.etCommentValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/{&quot;name&quot;:&quot;deep_purple_A200&quot;}"
            android:id="@+id/floatingBtnFloatingActionButton"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_50pxh"
            app:srcCompat="@drawable/img_group_9143" app:tint="@color/{&quot;name&quot;:&quot;primary&quot;}"/>
    </FrameLayout>
</layout>
